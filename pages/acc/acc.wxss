  align-items: center;
  background: #fff;
  width: 100%;
  height: 80rpx;

}
.first-form{
  border-radius:10rpx;
  margin-top: 100rpx;
  border:3rpx solid #eee;
  display: flex;
  align-items: center;
  background: #fff;
  width: 100%;
  height: 80rpx;
 }
.text-right{
  width: 100rpx;
  margin-right: 20rpx;
