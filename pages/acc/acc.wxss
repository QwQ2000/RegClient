page{
  background-color: #f5f5f5;
}
.titel{
  background: rgb(66, 65, 65);
  color: #fff;
 
  display: flex;
  align-items: center;
  justify-content: center;
  height: 90rpx;
  width: 100%;
}
.user-image{
  border-radius: 50%;
  width:200rpx;
  height: 200rpx;
  margin-left: 20rpx;
}
.user-icon{
 
  width:100rpx;
  height: 70rpx;
  margin-left: 20rpx;
}
.name-text{
  border:3rpx solid #eee;
  display: flex;
  background: #fff;
  width: 100%;
  margin-top: 100rpx;
  margin-left: auto;
  height:200rpx; 
  border-radius:10rpx;
}
.user-name{
  font-size-adjust: larger;
  display: flex;
  align-items: center;
  margin-left: 20px;
}
.form{
  border:3rpx solid #eee;
  border-radius:10rpx;
